{
  "name": "phpsafari/health-checks",
  "description": "Make health checks in your laravel application",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Visti KlÃ¸ft",
      "email": "hr.kloft@gmail.com"
    },
    {
      "name": "Peter Suhm",
      "email": "peter@suhm.dk"
    }
  ],
  "repositories": [
    {
        "type": "package",
        "package": {
            "name": "vistik/typed-collections",
            "version": "2.0.2",
            "source": {
                "url": "https://github.com/jason-klein/typed-collections",
                "type": "git",
                "reference": "dev-laravel6"
            },
            "type": "library",
            "autoload": {
              "psr-4": {
                "PhpSafari\\": "src/"
              },
              "files": [
                "src/helpers.php"
              ]
            }
        }
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "illuminate/support": "~6.0|~5.4|~5.3",
    "vistik/typed-collections": "^2.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "*",
    "phpunit/phpunit": "~8.0|~7.0|~6.0",
    "orchestra/testbench": "~4.0|~3.0",
    "mockery/mockery": "^1.2|^0.9.9",
    "phpmd/phpmd": "^2.6"
  },
  "autoload": {
    "psr-4": {
      "PhpSafari\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "PhpSafari\\ServiceProvider\\HealthCheckServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "phpunit",
    "check": [
      "./vendor/bin/phpmd src text codesize, controversial, design, naming, unusedcode",
      "./vendor/bin/phpcbf --standard=PSR2 src/"
    ]
  }
}
